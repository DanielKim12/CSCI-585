{"components/common/subscription-form":"<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\r\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" data-error-message=\"{{lang 'newsletter.newsletter_error'}}\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field1 form-field-custom\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap subscribeFormContent\">\r\n                <style>\r\n                  .form-input::placeholder {\r\n                    color: black;\r\n                    font-weight: 400;\r\n                  }\r\n                </style>\r\n                <input style=\"border: 3px solid black; border-radius: 10px; background-color: transparent; color: black;\"\r\n                  class=\"form-input\"\r\n                  id=\"nl_email\"\r\n                  name=\"nl_email\"\r\n                  type=\"email\"\r\n                  value=\"{{customer.email}}\"\r\n                  placeholder=\"Enter Email:\"\r\n                  onfocus=\"this.placeholder = ''\"\r\n                  onblur=\"this.placeholder = 'Enter email'\"\r\n                />\r\n                <input style=\"border-radius:10px;\" class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"}
